{
  "name": "feedfathom-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "export _UID=$(id -u) && export _GID=$(id -g) && docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "build-server": "vite build",
    "build-worker": "bun build ./src/worker-entrypoint.ts --outdir=./build/ --target=bun --sourcemap",
    "watch-server": "svelte-kit sync && vite dev --host",
    "watch-worker": "svelte-kit sync && bun --watch ./src/worker-entrypoint.ts",
    "lint": "bun test && svelte-kit sync && tsc && eslint ./src && prettier --check ./src && svelte-check --tsconfig ./tsconfig.json",
    "lint-tsc": "tsc",
    "lint-eslint": "eslint ./src",
    "lint-prettier": "prettier --check ./src",
    "lint-svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint:fix": "svelte-kit sync && eslint src --fix && prettier --write ./src",
    "format": "prettier --write ./src",
    "pack": "bin/generate-packages.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@types/archiver": "^6.0.3",
    "@types/bun": "^1.2.1",
    "@types/eslint": "^9.6.1",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/mailparser": "^3.4.5",
    "@types/smtp-server": "^3.5.10",
    "@types/xml2js": "^0.4.14",
    "@total-typescript/ts-reset": "^0.6.1",
    "archiver": "^7.0.1",
    "drizzle-kit": "0.30.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte-check": "^4.1.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "@eslint/js": "^9.19.0",
    "@extractus/article-extractor": "^8.0.16",
    "@mozilla/readability": "^0.5.0",
    "@rowanmanning/feed-parser": "^1.1.0",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "awilix": "^12.0.4",
    "axios": "^1.7.9",
    "axios-cache-interceptor": "^1.6.2",
    "bullmq": "^5.38.0",
    "dompurify": "^3.2.4",
    "drizzle-orm": "^0.39.0",
    "ioredis": "^5.4.2",
    "jsdom": "^26.0.0",
    "mailparser": "^3.7.2",
    "opml": "^0.5.7",
    "smtp-server": "^3.13.6",
    "svelte": "^5.19.3",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-preprocess": "^6.0.3",
    "ulid": "^2.3.0",
    "valibot": "^1.0.0-beta.14",
    "vite": "^6.0.11",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "type": "module",
  "trustedDependencies": []
}
