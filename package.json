{
  "name": "feedfathom-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "export _UID=$(id -u) && export _GID=$(id -g) && docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "build-server": "bunx --bun svelte-kit sync && bunx --bun vite build",
    "build-worker": "bun build ./src/worker-entrypoint.ts --outdir=./build/ --target=bun --sourcemap",
    "watch-server": "bunx --bun svelte-kit sync && bunx --bun vite dev --host",
    "watch-worker": "bunx --bun svelte-kit sync && bun --watch ./src/worker-entrypoint.ts",
    "lint": "bun test && bunx --bun svelte-kit sync && biome check && bunx --bun tsc && bunx --bun eslint && bunx --bun svelte-check --tsconfig ./tsconfig.json",
    "lint:fix": "bunx --bun svelte-kit sync && bunx --bun eslint --fix && biome check --write",
    "format": "biome format --write",
    "pack": "bin/generate-packages.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/archiver": "^6.0.3",
    "@types/bun": "^1.2.5",
    "@types/chrome": "^0.0.308",
    "@types/eslint": "^8.56.6",
    "@types/jsdom": "^21.1.7",
    "@types/mailparser": "^3.4.5",
    "@types/smtp-server": "^3.5.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "archiver": "^7.0.1",
    "drizzle-kit": "0.30.5",
    "eslint": "^8.56.0",
    "eslint-config-biome": "^1.9.4",
    "eslint-config-canonical": "^44.3.44",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-svelte": "^3.1.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "@eslint/js": "^8.56.0",
    "@extractus/article-extractor": "^8.0.17",
    "@mozilla/readability": "^0.6.0",
    "@rowanmanning/feed-parser": "^2.0.0",
    "@sveltejs/kit": "^2.19.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@worker-tools/html-rewriter": "^0.1.0-pre.19",
    "awilix": "^12.0.4",
    "axios": "^1.8.2",
    "axios-cache-interceptor": "^1.6.2",
    "bullmq": "^5.41.8",
    "dompurify": "^3.2.4",
    "drizzle-orm": "^0.40.0",
    "fast-xml-parser": "^5.0.8",
    "ioredis": "^5.6.0",
    "jsdom": "^26.0.0",
    "mailparser": "^3.7.2",
    "opml": "^0.5.7",
    "smtp-server": "^3.13.6",
    "svelte": "^5.22.6",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-preprocess": "^6.0.3",
    "ulid": "^2.3.0",
    "valibot": "^1.0.0-rc.3",
    "vite": "^6.2.1",
    "xml2js": "^0.6.2"
  },
  "type": "module",
  "trustedDependencies": []
}
