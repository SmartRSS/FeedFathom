{
  "name": "feedfathom-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "export _UID=$(id -u) && export _GID=$(id -g) && docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "build-server": "bunx --bun svelte-kit sync && bunx --bun vite build",
    "build-worker": "bun build ./src/worker-entrypoint.ts --outdir=./build/ --target=bun --sourcemap",
    "watch-server": "bunx --bun svelte-kit sync && bunx --bun vite dev --host",
    "watch-worker": "bunx --bun svelte-kit sync && bun --watch ./src/worker-entrypoint.ts",
    "lint": "bun test && bunx --bun svelte-kit sync && bunx --bun tsc && bunx --bun eslint ./src && bunx --bun svelte-check --tsconfig ./tsconfig.json && bunx --bun prettier --check ./src",
    "lint:fix": "bunx --bun svelte-kit sync && bunx --bun eslint src --fix && bunx --bun prettier --write ./src",
    "format": "prettier --write ./src",
    "pack": "bin/generate-packages.ts"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/archiver": "^6.0.3",
    "@types/bun": "^1.2.3",
    "@types/chrome": "^0.0.306",
    "@types/eslint": "^9.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/mailparser": "^3.4.5",
    "@types/smtp-server": "^3.5.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "archiver": "^7.0.1",
    "drizzle-kit": "0.30.5",
    "eslint": "^9.21.0",
    "eslint-config-canonical": "^44.3.43",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte-check": "^4.1.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "@eslint/js": "^9.21.0",
    "@extractus/article-extractor": "^8.0.17",
    "@mozilla/readability": "^0.5.0",
    "@rowanmanning/feed-parser": "^1.1.1",
    "@sveltejs/kit": "^2.17.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "awilix": "^12.0.4",
    "axios": "^1.7.9",
    "axios-cache-interceptor": "^1.6.2",
    "bullmq": "^5.41.5",
    "dompurify": "^3.2.4",
    "drizzle-orm": "^0.39.3",
    "fast-xml-parser": "^5.0.7",
    "ioredis": "^5.5.0",
    "jsdom": "^26.0.0",
    "mailparser": "^3.7.2",
    "opml": "^0.5.7",
    "smtp-server": "^3.13.6",
    "svelte": "^5.20.4",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-preprocess": "^6.0.3",
    "ulid": "^2.3.0",
    "valibot": "^1.0.0-rc.2",
    "vite": "^6.2.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "xml2js": "^0.6.2"
  },
  "type": "module",
  "trustedDependencies": []
}
