{
  "name": "feedfathom-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "export _UID=$(id -u) && export _GID=$(id -g) && docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "build-extension": "bin/generate-packages.ts",
    "build-server": "bunx --bun svelte-kit sync && bunx --bun vite build",
    "build-worker": "bun build ./src/worker-entrypoint.ts --outdir=./build/ --target=bun --sourcemap",
    "build-for-image": "mkdir -p build && bun build-server && bun build-worker",
    "build-project": "rm -rf build && mkdir -p build && (bun build-server & bun build-worker & bun build-extension & wait)",
    "watch-server": "bunx --bun svelte-kit sync && bunx --bun vite dev --host",
    "watch-worker": "bunx --bun svelte-kit sync && bun --watch ./src/worker-entrypoint.ts",
    "eslint": "bunx --bun eslint --config=eslint.config.js ./src/**/*.{ts,svelte} ./tests/**/*.ts",
    "eslint:fix": "bunx --bun eslint --config=eslint.config.js ./src/**/*.{ts,svelte} ./tests/**/*.ts --fix",
    "svelte-check": "bunx --bun svelte-check --tsconfig ./tsconfig.json",
    "lint": "bun test && bunx --bun svelte-kit sync && biome check && bunx --bun tsc && bun eslint && bun svelte-check",
    "lint:fix": "bunx --bun svelte-kit sync && bun eslint:fix --fix && biome check --write",
    "format": "biome format --write"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/archiver": "^6.0.3",
    "@types/bun": "^1.2.6",
    "@types/chrome": "^0.0.310",
    "@types/eslint": "^9.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/mailparser": "^3.4.5",
    "@types/smtp-server": "^3.5.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "archiver": "^7.0.1",
    "drizzle-kit": "0.30.5",
    "eslint": "^9.23.0",
    "eslint-config-biome": "^1.9.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-svelte": "^3.3.3",
    "globals": "^16.0.0",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "@eslint/js": "^9.23.0",
    "@extractus/article-extractor": "^8.0.17",
    "@mozilla/readability": "^0.6.0",
    "@rowanmanning/feed-parser": "^2.0.1",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "arktype": "^2.1.12",
    "awilix": "^12.0.5",
    "axios": "^1.8.4",
    "axios-cache-interceptor": "^1.7.0",
    "bullmq": "^5.44.4",
    "dompurify": "^3.2.4",
    "drizzle-orm": "^0.41.0",
    "fast-xml-parser": "^5.0.9",
    "ioredis": "^5.6.0",
    "jsdom": "^26.0.0",
    "mailparser": "^3.7.2",
    "opml": "^0.5.7",
    "smtp-server": "^3.13.6",
    "svelte": "^5.25.3",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-preprocess": "^6.0.3",
    "ulid": "^3.0.0",
    "vite": "^6.2.3",
    "xml2js": "^0.6.2"
  },
  "type": "module",
  "trustedDependencies": []
}
