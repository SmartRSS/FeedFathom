{
  "name": "feedfathom-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "docker build -t feedfathom . -f Dockerfile && docker compose -f docker-compose.yml -f dev-server.yml up -d",
    "build-server": "vite build",
    "build-worker": "bun build ./src/worker-entrypoint.ts --outdir=./build/ --target=bun --sourcemap",
    "preview": "vite preview",
    "preview:build": "vite build && vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "concurrently \"bun:lint-*\"",
    "lint-tsc": "tsc",
    "lint-eslint": "eslint ./src",
    "lint-prettier": "prettier --check ./src",
    "lint-svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint:fix": "prettier --write ./src && eslint src --fix && svelte-kit sync",
    "format": "prettier --write ./src",
    "vite:dev": "vite dev --host",
    "pack": "bin/generate-packages.ts",
    "start": "docker compose up -d --remove-orphans",
    "sync": "export NODE_ENV=development && svelte-kit sync"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/archiver": "^6.0.3",
    "@types/bun": "^1.2.0",
    "@types/cheerio": "^0.22.35",
    "@types/eslint": "^9.6.1",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/mailparser": "^3.4.5",
    "@types/smtp-server": "^3.5.10",
    "@types/xml2js": "^0.4.14",
    "concurrently": "^9.1.2",
    "globals": "^15.14.0",
    "svelte": "^5.19.2",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-preprocess": "^6.0.3",
    "vite": "^6.0.11",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "archiver": "^7.0.1",
    "drizzle-kit": "0.30.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte-check": "^4.1.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@rowanmanning/feed-parser": "^1.1.0",
    "awilix": "^12.0.4",
    "axios": "^1.7.9",
    "axios-cache-interceptor": "^1.6.2",
    "bullmq": "^5.35.1",
    "cheerio": "^1.0.0",
    "drizzle-orm": "^0.38.4",
    "ioredis": "^5.4.2",
    "jsdom": "^26.0.0",
    "mailparser": "^3.7.2",
    "opml": "^0.5.7",
    "postgres": "^3.4.5",
    "smtp-server": "^3.13.6",
    "ulid": "^2.3.0",
    "zlib": "^1.0.5",
    "zod": "^3.24.1"
  },
  "type": "module",
  "trustedDependencies": [
    "@sveltejs/kit",
    "core-js",
    "esbuild",
    "msgpackr-extract",
    "puppeteer",
    "svelte-preprocess"
  ]
}
